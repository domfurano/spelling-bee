:28}},{"source":"systems/text-generation.system.ts","name":null,"original":{"line":4,"column":13},"generated":{"line":82,"column":31}},{"source":"systems/text-generation.system.ts","name":null,"original":{"line":4,"column":13},"generated":{"line":82,"column":51}}],"sources":{"systems/text-generation.system.ts":"import {Engine, Family, FamilyBuilder, System} from \"@mesa-engine/core\";\nimport {InputComponent, TextComponent} from \"../components\";\n\nexport class TextGenerationSystem extends System {\n  inputFamily: Family;\n  ctx: CanvasRenderingContext2D;\n  canvas: HTMLCanvasElement;\n  word: string;\n\n  constructor() {\n    super();\n    this.word = TextGenerationSystem.shuffleWord('ALCOVES');\n    this.canvas = document.getElementById('canvas');\n    this.ctx = <CanvasRenderingContext2D>this.canvas.getContext('2d');\n  }\n\n  onAttach(engine: Engine) {\n    super.onAttach(engine);\n    this.inputFamily = new FamilyBuilder(engine).include(InputComponent).build();\n    for (let i = 0; i < this.inputFamily.entities.length; i++) {\n      let entity = this.inputFamily.entities[i];\n      if (entity.hasComponent(TextComponent)) {\n        const text = entity.getComponent(TextComponent);\n        text.text = this.word[i];\n      }\n    }\n  }\n\n  update(engine: Engine, delta: number) {\n  }\n\n  private static shuffleWord(word: string) {\n    let array = word.split('');\n    for (let i = array.length - 1; i > 0; i--) {\n      let j = Math.floor(Math.random() * (i + 1));\n      let temp = array[i];\n      array[i] = array[j];\n      array[j] = temp;\n    }\n    return array.join('');\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"ec7f3ee6b6f40639e5987693e6930082","cacheData":{"env":{}}}